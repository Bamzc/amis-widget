{
  "name": "amis-widget",
  "version": "2.0.3",
  "description": "amis组件注册器（支持react、vue2.0和jQuery技术栈），用于注册amis渲染器、Editor插件",
  "keywords": [
    "amis组件注册器",
    "amis自定义组件",
    "amis-editor自定义插件"
  ],
  "author": "fex",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "amis dev",
    "build": "amis build",
    "build2lib": "amis build2lib",
    "build2esm": "amis build2esm",
    "format": "prettier --write \"src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\"",
    "publish2local": "sh publish-local.sh"
  },
  "files": [
    "dist/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://git@github.com:aisuda/amis-widget.git"
  },
  "bugs": {
    "url": "https://github.com/aisuda/amis-widget/issues"
  },
  "dependencies": {
    "amis": "2.0.0-rc.16",
    "amis-editor": "4.2.0-beta.3",
    "axios": "^0.24.0",
    "jquery": "^3.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass-loader": "^13.0.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jquery": "^3.5.14",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^17.0.13",
    "amis-widget-cli": "^2.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "monaco-editor-webpack-plugin": "6.0.0",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  }
}
